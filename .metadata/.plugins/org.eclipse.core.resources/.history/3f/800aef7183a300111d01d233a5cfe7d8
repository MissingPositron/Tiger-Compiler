package syntaxtree;import visitor.InterpreterVisitor;import visitor.Visitor;import visitor.TypeVisitor;public class IntegerLiteral extends Exp {  public int i;  public IntegerLiteral(int ai, Position p) {    i=ai;    position = p;  }  public void accept(Visitor v) {    v.visit(this);  }    public Object accept(InterpreterVisitor v) {	  return v.visit(this);  }  public Type accept(TypeVisitor v) {	      return v.visit(this);  }}